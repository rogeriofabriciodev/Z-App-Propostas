                <select class="form-control" id="formControlSelectProdutosPorCategoria">
                  
                </select>
              </div>

              <div class="form-group col-2">
                <label for="inputQuantity">Quantidade</label>
                <input type="number" class="form-control" id="inputQuantity">
              </div>

              <div class="form-group col-2 pt-3">
                <button id="btn-add-product" type="button" class="btn btn-primary mt-2">Adicionar</button>
              </div>
            </div>

          </form>
          <!-- Fim do form -->
          <hr></hr>
          <div class="col-12">
            <div class="d-flex justify-content-between align-items-center"><span class="col-8">Descrição</span><span class="col-2 text-center">Quant.</span>
              <span class="col-2 text-center">Remover</span>
            </div>
            <hr></hr>
            <ul id="list-products-solutions-proposal-projects-customers" class="list-group list-group-flush" style="list-style-type: none;">
              
            </ul>
            <div class="d-flex justify-content-between align-items-center mt-3">
              <span class="col-8"></span>
              <span class="col-2 text-center">Sub-Total</span>
              <span id="subtotal" class="col-2 text-center"></span>
            </div>
          </div>
          <!-- Fim da List -->
        </div>
        <div class="modal-footer">
          <button type="button" class="btn btn-success" id="btn-new-product" data-dismiss="modalNewProduct">
            Soluções
          </button>
        </div>

      </div>
    </div>
