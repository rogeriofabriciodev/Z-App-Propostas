  <!--Modal Form Customer-->
  <div class="modal fade bd-example-modal-lg" id="modal-add-customers" tabindex="-1" role="dialog">
    <div class="modal-dialog modal-lg" role="document">
      <div class="modal-content">

        <div class="modal-header">
          <h1 class="h3 mb-4 text-gray-800">Cadastro do Cliente</h1>
          <button type="button" class="btn-close-modal-add" data-dismiss="modal">
            <span>&times;</span>
          </button>
        </div>
        <div class="modal-body">

          <!-- Inicio do FORM -->

          <section id="content-section">
            <div class="row">
              <div class="col-12">

                <!-- Begin Page Content -->
                <div class="container-fluid">

                  <!-- Page Heading -->
                  <div>
                    <form id="form-add-customer">
                      <div class="form-row">
                        <div class="form-group col-md-8">
                          <label for="inputName">Nome Completo</label>
                          <input type="text" class="form-control" id="inputName" placeholder="Nome Completo">
                        </div>
                        <div class="form-group col-md-4">
                          <label for="inputCpf">CPF</label>
                          <input type="text" class="form-control" id="inputCpf" placeholder="Apenas Números">
                        </div>
                      </div>
                      <div class="form-row">
                        <div class="form-group col-md-4">
                          <label for="inputCelular">Celular</label>
                          <input type="text" class="form-control" id="inputCelular" placeholder="Apenas número com DDD">
                        </div>
                        <div class="form-group col-md-8">
                          <label for="inputEmail">Email</label>
                          <input type="email" class="form-control" id="inputEmail" placeholder="Email">
                        </div>
                      </div>
                      <div class="form-group">
                        <label for="inputAddress">Endereço</label>
                        <input type="text" class="form-control" id="inputAddress"
                          placeholder="Logradouro / Número / Bairro">
                      </div>
                      <div class="form-group">
                        <label for="inputAddress2">Complemento</label>
                        <input type="text" class="form-control" id="inputAddress2"
                          placeholder="Edifício / Apartamento / etc">
                      </div>
                      <div class="form-row">
                        <div class="form-group col-md-6">
                          <label for="inputCity">Cidade</label>
                          <input type="text" class="form-control" id="inputCity" placeholder="Nome da cidade">
                        </div>
                        <div class="form-group col-md-4">
                          <label for="inputState">Estado</label>
                          <select id="inputState" class="form-control">
                            <option selected>Escolha...</option>
                            <option>BA</option>
                            <option>PE</option>
                          </select>
                        </div>
                        <div class="form-group col-md-2">
                          <label for="inputZip">CEP</label>
                          <input type="text" class="form-control" id="inputZip" placeholder="Apenas números">
                        </div>
                      </div>
                      <div class="form-group">
                        <button id="btn-save-customer" type="button" class="btn btn-success mt-2">Salvar</button>
                    </form>
                  </div>

                </div>
                <!-- /.container-fluid -->

              </div>
              <!-- End of Main Content -->

            </div>
          </section>

          <!-- Fim do FORM -->
