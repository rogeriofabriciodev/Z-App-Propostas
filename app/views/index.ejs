  <!--Modal List Product-->
  <div class="modal fade bd-example-modal-lg" id="modal-list-products" tabindex="-1" role="dialog">
    <div class="modal-dialog modal-lg modal-dialog-scrollable" role="document">
      <div class="modal-content">

        <div class="modal-header">
          <h6 class="h6 mb-4 text-gray-800" id="product-title"></h6>
          <button type="button" class="btn-modal-close-product" data-dismiss="modalListProduct">
            <span>&times;</span>
          </button>
        </div>
        <div class="modal-body">

          <form id="form-add-products">

            <div class="form-row">
